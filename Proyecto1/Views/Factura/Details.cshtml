@model Proyecto1.Models.Factura

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles de Factura</h1>

<div>
    <h4>Factura</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2 my-3">
            ID
        </dt>
        <dd class="col-sm-10 my-3">
            @Html.DisplayFor(model => model.idFactura)
        </dd>
        <dt class="col-sm-2 my-3">
            Parqueo
        </dt>
        <dd class="col-sm-10 my-3">
            @ViewBag.parqueo.nombre
        </dd>
        <dt class="col-sm-2 my-3">
            Valor
        </dt>
        <dd class="col-sm-10 my-3">
            @Html.DisplayFor(model => model.valor)
        </dd>
        <dt class="col-sm-2 my-3">
            Fecha
        </dt>
        <dd class="col-sm-10 my-3">
            @Html.DisplayFor(model => model.fecha)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Regresar</a>
</div>
