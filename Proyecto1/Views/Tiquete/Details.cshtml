@model Proyecto1.Models.Tiquete

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del tiquete</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2 my-3">
            ID
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.id)
        </dd>
        <dt class = "col-sm-2 my-3">
            Hora de Ingreso
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.ingreso)
        </dd>
        <dt class = "col-sm-2 my-3">
            Hora de Salida
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.salida)
        </dd>
        <dt class = "col-sm-2 my-3">
            Placa
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.placa)
        </dd>
        <dt class = "col-sm-2 my-3">
            Parqueo
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.idParqueo)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.id }) |
    <a asp-action="Index">Regresar</a>
</div>
