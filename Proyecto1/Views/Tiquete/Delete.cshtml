@model Proyecto1.Models.Tiquete

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>¿Desea eliminar este tiquete?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2 my-3">
            ID
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.id)
        </dd>
        <dt class = "col-sm-2 my-3">
            Hora de Ingreso
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.ingreso)
        </dd>
        <dt class = "col-sm-2 my-3">
            Hora de Salida
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.salida)
        </dd>
        <dt class = "col-sm-2 my-3">
            Placa
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.placa)
        </dd>
        <dt class = "col-sm-2 my-3">
            Parqueo
        </dt>
        <dd class = "col-sm-10 my-3">
            @Html.DisplayFor(model => model.idParqueo)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Regresar</a>
    </form>
</div>
